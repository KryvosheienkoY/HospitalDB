<html>
<%- include('head'); %>
<body>
<%- include('toolbar_admin_view',{section: 'admin'}); %>

<div>
    <p type="text" id="getID" style="display: none"><%= token %></p>
    <button class="btn  btn-sm btn-primary" onclick="sendAjaxPatientTable()">Таблиця пацієнтів</button>

</div>
<script>
    function sendAjaxPatientTable(){

        console.log("token -" + sessionStorage.getItem('token'));
        $.ajax({
            type: 'GET',
            headers: {Authorization: sessionStorage.getItem("token")},
            url: '/admin/patient_table',
            success: function (response) {
                console.log("/admin/patient_table");
                $("body").html(response);
            }
        });
    }
</script>
<script>
    $(document).ready(function () {
        let t=$('#getID').html();
        sessionStorage.setItem('token',t);
    });
</script>
</body>
</html>